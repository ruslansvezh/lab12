#include <iostream>
#include <cmath>
using namespace std;
int Sign(double X)
{
	if (X < 0)
		return -1;
	else if (X == 0)
		return 0;
	else
		return 1;
}
int main() 
{
	double A, B;
	setlocale(0, "");
	cout << "Введите А: " << endl;
	cin >> A;
	cout << "Введите В: " << endl;
	cin >> B;
	cout << Sign(A) + Sign(B);
}
